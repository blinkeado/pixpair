{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "users": {
      "$userId": {
        ".read": "auth != null && auth.uid === $userId",
        ".write": "auth != null && auth.uid === $userId",
        "photos": {
          "$photoId": {
            ".read": "auth != null",
            ".write": "auth != null && auth.uid === $userId"
          }
        },
        "sessions": {
          "$sessionId": {
            ".read": "auth != null",
            ".write": "auth != null && auth.uid === $userId",
            "photoIds": {
              "$photoId": {
                ".read": "auth != null",
                ".write": "auth != null && auth.uid === $userId"
              }
            }
          }
        }
      }
    },
    "sessions": {
      "$sessionId": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid != null",
        "members": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid != null"
        },
        "photos": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid != null"
        },
        "capture": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid != null"
        },
        "pictures": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid != null"
        },
        "participants": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid != null"
        }
      }
    },
    "test": {
      ".read": true,
      ".write": true
    }
  }
} 